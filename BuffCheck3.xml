    <Frame name="BuffCheck3ConsumeList" toplevel="true" movable="true" parent="UIParent" enableMouse="true" hidden="true" mixin="BuffCheck3">
        <TitleRegion setAllPoints="true"/>
        <Size>
            <AbsDimension x="480" y="60" />
