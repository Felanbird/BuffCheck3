<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Script file="BuffCheck3.lua"/>

    <Button name="BuffCheck3ButtonTemplate" hidden="true" virtual="true" enableMouse="true">
        <Size>
            <AbsDimension x="32" y="32"/>
        </Size>
        <Scripts>
            <OnLoad>
                self:RegisterForClicks("RightButtonDown")
                getglobal(self:GetName().."Highlight"):Hide()
                GameTooltip:Hide()
            </OnLoad>
            <OnClick>
                BuffCheck3:ButtonOnClick(self.consume)
            </OnClick>
            <OnEnter>
                getglobal(self:GetName().."Highlight"):Show()
                if self.texture ~= "Interface\\Icons\\Spell_Nature_WispSplode" then
                    BuffCheck3:ShowTooltip(self.consume, self:GetID())
                end
            </OnEnter>
            <OnLeave>
                getglobal(self:GetName().."Highlight"):Hide()
                GameTooltip:Hide()
                if not SpellIsTargeting() then
                    BuffCheck3:HideWeaponButtons()
                end
            </OnLeave>
        </Scripts>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture name="$parentIcon"/>
            </Layer>
            <Layer level="ARTWORK">
                <FontString name="$parentCount" font="GameFontNormal" justifyH="LEFT">
                    <Size>
                        <AbsDimension x="20" y="20"/>
                    </Size>
                    <Anchors>
                        <Anchor point="LEFT" relativeTo="$parentIcon" relativePoint="RIGHT">
                            <Offset>
                                <AbsDimension x="-10" y="-10"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <FontString name="$parentDuration" font="GameFontNormal" justifyH="LEFT">
                    <Size>
                        <AbsDimension x="20" y="20"/>
                    </Size>
                    <Anchors>
                        <Anchor point="LEFT" relativeTo="$parentIcon" relativePoint="RIGHT">
                            <Offset>
                                <AbsDimension x="-25" y="2"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                </FontString>
                <Texture name="$parentHighlight" file="Interface\QuestFrame\UI-QuestLogTitleHighlight" hidden="false" alphaMode="ADD">
                    <Size>
                        <AbsDimension x="32" y="32"/>
                    </Size>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentIcon"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Button>

    <Button name="BuffCheck3WeaponButtonTemplate" virtual="true" hidden="true" enableMouse="true" toplevel="true">
        <Size>
            <AbsDimension x="32" y="32"/>
        </Size>
        <Scripts>
            <OnLoad>
                    getglobal(self:GetName().."Highlight"):Hide()
                    GameTooltip:Hide()
            </OnLoad>
            <OnClick>
                    BuffCheck3:WeaponButtonOnClick(self:GetID())
            </OnClick>
            <OnEnter>
                    getglobal(self:GetName().."Highlight"):Show()
                    BuffCheck3:ShowWeaponTooltip(self:GetID())
            </OnEnter>
            <OnLeave>
                    getglobal(self:GetName().."Highlight"):Hide()
                    GameTooltip:Hide()
                    if not SpellIsTargeting() then
                        BuffCheck3:HideWeaponButtons()
                    end
            </OnLeave>
        </Scripts>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture name="$parentIcon"/>
            </Layer>
            <Layer level="ARTWORK">
                <Texture name="$parentHighlight" file="Interface\QuestFrame\UI-QuestLogTitleHighlight" hidden="false" alphaMode="ADD">
                    <Size>
                        <AbsDimension x="32" y="32"/>
                    </Size>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeTo="$parentIcon"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Button>

